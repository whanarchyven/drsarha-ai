{
  "name": "drsarha-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "NODE_OPTIONS=--max_old_space_size=8192 next dev",
    "dev:backend": "convex dev",
    "predev": "convex dev --until-success && convex dev --once --run-sh \"node setup.mjs --once\" && convex dashboard",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ignore-pattern \"convex/_generated/**\""
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.90",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "convex-helpers": "^0.1.113",
    "json5": "^2.2.3",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-simple-code-editor": "^0.14.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "^1.1.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.11",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.4",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
